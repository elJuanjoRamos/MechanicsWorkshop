                              <HBox alignment="CENTER_RIGHT" prefHeight="53.0" prefWidth="473.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <JFXButton onAction="#open_Dialog" prefHeight="31.0" prefWidth="117.0" styleClass="success-buttton" text="Add">
