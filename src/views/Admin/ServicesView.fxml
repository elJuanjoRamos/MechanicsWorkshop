                              <TableColumn fx:id="mark" maxWidth="223.0" minWidth="0.0" prefWidth="166.0" text="MARK" />
                              <TableColumn fx:id="model" maxWidth="317.0" minWidth="0.0" prefWidth="177.0" text="MODEL" />
